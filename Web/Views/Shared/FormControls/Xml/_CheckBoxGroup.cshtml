@{
    Layout = "";
    BussinessModels.fitem control = (BussinessModels.fitem)ViewData["FormControl"];
}

@if (control != null)
{
    <div class="col-lg-4 col-md-6 col-sm-12">
        <div class="form-group row">
            <label class="col-12 txt-left col-form-label">@control.display</label>
            <div class="col-12">
                <div class="checkbox-fade fade-in-primary">
                    <label for="@(control.key)_1">
                        <input type="checkbox" name="check1" id="@(control.key)_1" value="@control.value">
                        <span class="cr">
                            <i class="cr-icon fa fa-check txt-primary"></i>
                        </span>
                        <span>@control.display</span>
                    </label>
                </div>
                <div class="checkbox-fade fade-in-primary">
                    <label for="@(control.key)_2">
                        <input type="checkbox" name="check1" id="@(control.key)_2" value="@control.value">
                        <span class="cr">
                            <i class="cr-icon fa fa-check txt-primary"></i>
                        </span>
                        <span>@control.display</span>
                    </label>
                </div>
            </div>
        </div>
    </div>
}

